{
	"port": @%@portal/port@%@,
	"admin_groups": @!@
import json
from univention.lib.misc import custom_groupname
default_admin_groups = custom_groupname("Domain Admins", configRegistry) + ",cn=groups," + configRegistry.get("ldap/base")
admin_groups = configRegistry.get("portal/admin-groups", default_admin_groups).split(", ")
admin_groups = [group.strip() for group in admin_groups]
print(json.dumps(admin_groups), end="")
	@!@,
	"fqdn": "@%@hostname@%@.@%@domainname@%@",
	"hostdn": "@%@ldap/hostdn@%@",
	"ldap_base": "@%@ldap/base@%@",
	"ldap_uri": "ldap://@%@ldap/server/name@%@:@%@ldap/server/port@%@",
	"umc_session_url": "http://@%@umc/http/interface@%@:@%@umc/http/port@%@/get/session-info",
	"default_domain_dn": "cn=domain,cn=portal,cn=portals,cn=univention,@%@ldap/base@%@"
}
