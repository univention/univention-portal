{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.2.1",
    "generators": {
      "v2.0": {
        "generatorName": "typescript-axios",
        "output": "src/apis/notifications",
        "inputSpec": "http://localhost:8080/openapi.json",
        "additionalProperties": {
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}